@using Test.Web.Components.Layout
@using Test.Web.Components.Shared
<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="[typeof(Test.Web.Client._Imports).Assembly]">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(MainLayout)">
            <NotAuthorized>
                <RedirectToLogin/>
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate Selector="h1" RouteData="routeData"/>
    </Found>
</Router>